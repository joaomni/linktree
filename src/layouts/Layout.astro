<style is:global>
  @import url("https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap");
  :root {
    --accent: 136, 58, 234;
    --accent-light: 224, 204, 250;
    --accent-dark: 49, 10, 101;
  }
  * {
    margin: 0;
    padding: 0;
    line-height: 1;
    list-style-type: none;
    text-decoration: none;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
    box-sizing: border-box;
    -webkit-font-smoothing: antialiased;
    max-width: 100%;
    overflow-wrap: break-word;
  }
  html {
    font-family: "Roboto", sans-serif;
    font-weight: 400;
    font-style: normal;
    scroll-behavior: smooth;
  }
  body {
    background-color: #0c2718;
    color: #ffffff;
  }
  .trasition {
    transition:
      box-shadow 0.25s cubic-bezier(0.08, 0.59, 0.29, 0.99) 0s,
      color 0.25s cubic-bezier(0.08, 0.59, 0.29, 0.99) 0s,
      border-color 0.25s cubic-bezier(0.08, 0.59, 0.29, 0.99) 0s,
      transform 0.25s cubic-bezier(0.08, 0.59, 0.29, 0.99) 0s,
      background-color 0.25s cubic-bezier(0.08, 0.59, 0.29, 0.99) 0s;
  }
  img,
  svg {
    transition: transform 0.5s ease; /* dura√ß√£o e efeito da transi√ß√£o */
  }
  svg:hover {
    transform: scale(1.2); /* aumenta a imagem em 20% */
  }
  img:hover {
    transform: scale(1.2); /* aumenta a imagem em 20% */
  }
</style>
